{
  "name": "rummy-backend",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --respawn src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "smoke": "ts-node src/smoke.ts",
    "test": "jest",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "bot": "ts-node src/dev-bot.ts",
    "create-user": "ts-node src/scripts/create-user.ts",
    "reconcile": "ts-node src/scripts/reconcile.ts",
    "lint": "eslint 'src/**/*.{ts,tsx}' --max-warnings=0",
    "backfill": "ts-node src/scripts/backfill_wallets.ts"
  },
  "dependencies": {
    "@socket.io/redis-adapter": "^8.3.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.7.0",
    "mongoose": "^8.6.3",
    "morgan": "^1.10.0",
    "socket.io": "^4.7.5",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.6",
    "@types/node": "^20.11.30",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "eslint": "^8.57.0"
  }
}
